<Window x:Class="GeoLib.WindowsHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Name="btnStart" FontSize="20" Content="Start services" Margin="15 15 15 15" Click="btnStart_Click"></Button>
            <Button Name="btnStop" FontSize="20" Content="Stop services" Margin="15 15 15 15" Click="btnStop_Click" IsEnabled="False"></Button>
            <Label Name="lblText" FontSize="20" Margin="15 15 15 15"></Label>
        </StackPanel>
    </Grid>
</Window>
